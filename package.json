{
  "name": "gh-projects",
  "version": "0.0.1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "md-projects": "node --no-warnings=ExperimentalWarning --loader ts-node/esm.mjs src/md-projects.ts",
    "dev": "nollup --environment NODE_ENV:development -c -w",
    "dev-build": "rollup --environment NODE_ENV:development -c",
    "build": "rollup --environment NODE_ENV:production -c",
    "test": "jest",
    "lint": "eslint .",
    "pretty-pug": "prettier --write \"**/*.pug\"",
    "wipe": "bash ./scripts/wipe.sh",
    "postinstall": "bash ./scripts/init.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@prettier/plugin-pug": "^1.13.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-strip": "^2.0.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "nollup": "^0.14.1",
    "prettier": "^2.2.1",
    "pug": "^3.0.0",
    "rollup": "^2.34.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "typescript": "~4.1.2"
  },
  "dependencies": {
    "@octokit/graphql": "^4.5.8",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0"
  }
}
